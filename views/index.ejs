<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css">
    <title><%= dir.nombre %></title>
</head>
<body>
    <ul class="nav justify-content-center">
        <% estructura.forEach(dir => {%>
        <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/<%= dir.nombre %> "><%= dir.nombre %></a>
        </li>
        <% }) %>
    </ul>
    <div class="container-fluid">
            <% dir.archivos.forEach(archivo => {%>
                <div class="card" id="<% archivo %> ">
                    <div class="card-body">
                        <h5 class="card-title"><%= dir.archivos %></h5>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</body>
</html>